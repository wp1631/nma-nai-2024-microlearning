        "    plt.show()\n",
        "\n",
        "# plot the SNR at initialization for the three learning algorithms\n",
        "with plt.xkcd():\n",
        "    plt.figure()\n",
        "    x = [0, 1, 2]\n",
        "    snr_vals = [snr_perturb, snr_node_perturb, snr_backprop]\n",
        "    colors = ['b', 'c', 'r']\n",
